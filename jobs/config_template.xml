<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
      <!-- only one trigger can exits -->
    <hudson.triggers.TimerTrigger>
        <!-- variable -->
        <spec>0 18-23 * * *</spec>
        <!--
            1. format:
            MINUTE (0-59), HOUR (0-23), DOM(day of month) (1-31), MONTH (1-12), DOW(DAY OF THE WEEK) (0-7)
            2. multiple times: ','.
            3. *: no limit.
            SAMPLE:
            from Monday to Friday, at 6:00am: 0 6 * * 1,2,3,4,5
            from 18:00-23:00 every day: 0 18-23 * * *
            daily at 08:30 in the morning, Monday - Friday: 30 08 * * 1-5
            weekday daily build twice a day, at lunchtime 12:00 and midnight 00:00, Sunday to Thursday: 00 0,12 * * 0-4
            daily in the late afternoon between 4:00 p.m. - 4:59 p.m. or 16:00 -16:59 depending on the projects hash: H 16 * * 1-
            midnight, every Saturday: @midnight or 59 23 * * 6
            every 1st day of every month between 2:00 a.m. - 02:30 a.m. : H(0,30) 02 01 * *

            can newline with one more schedules. but don't use <br>, just enter here is okay.
        -->
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
        <!-- variable -->
        <command>/usr/src/app/exec_tasks.sh updateDividend</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>
